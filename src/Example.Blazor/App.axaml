<local:App xmlns="https://github.com/avaloniaui"
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           x:Class="Example.Blazor.App"
           xmlns:local="using:Example.Blazor"
           xmlns:theme="clr-namespace:Consolonia.Themes;assembly=Consolonia.Themes"
           RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <local:App.Styles>
        <theme:MaterialTheme />
    </local:App.Styles>
</local:App>