<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="Consolonia.Gallery.Gallery.GalleryViews.GalleryImage">

    <TabControl TabStripPlacement="Left">
        <TabItem Header="0.jpg">
            <Image Source="avares://Consolonia.Gallery/Resources/0.jpg"
                   Stretch="Uniform">
                <Image.RenderTransform>
                    <!-- aspect ratio for most terminal fonts is 2:1 It's not knowable what the font is, but this is a reasonable approximation -->
                    <ScaleTransform ScaleX="2"
                                    ScaleY="1" />
                </Image.RenderTransform>
            </Image>
        </TabItem>
