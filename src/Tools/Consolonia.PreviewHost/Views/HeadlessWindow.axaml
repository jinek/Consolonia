<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" 
        x:Class="Consolonia.PreviewHost.Views.HeadlessWindow"
        KeyDown="OnKeyDown"
        Title="PreviewHost">
    <ContentControl DataContext="{Binding Project.Current}" Content="{Binding Content}" DataContextChanged="ContentControl_DataContextChanged"/>
</Window>
