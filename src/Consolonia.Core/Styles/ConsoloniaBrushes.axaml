<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:drawing="clr-namespace:Consolonia.Core.Drawing"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime">
    <system:Double x:Key="FontSizeNormal">1</system:Double>
    <drawing:FourBitColorBrush x:Key="ThemeForegroundBrush" Color="White"/>
    <drawing:FourBitColorBrush x:Key="ThemeBackgroundBrush" Color="Black"/>
    <drawing:FourBitColorBrush x:Key="ThemeAlternativeBackgroundBrush" Color="DarkGray"/>
    <drawing:FourBitColorBrush x:Key="ThemeBorderBrush" Color="Gray"/>
    <drawing:FourBitColorBrush x:Key="ThemeAccentForegroundBrush" Color="DarkCyan"/>
    <drawing:FourBitColorBrush x:Key="ThemeAccentBackgroundBrush" Color="Green"/>
</ResourceDictionary>
