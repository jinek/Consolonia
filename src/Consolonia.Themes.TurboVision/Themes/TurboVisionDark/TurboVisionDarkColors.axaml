<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:drawing="clr-namespace:Consolonia.Core.Drawing;assembly=Consolonia.Core">
    <!--https://www.mikrocontroller.net/attachment/483578/Turbo_Vision.gif-->
    <drawing:FourBitColorBrush x:Key="ThemeForegroundBrush" Color="White"/>
    <drawing:FourBitColorBrush x:Key="ThemeBackgroundBrush" Color="DarkGray"/>
    <drawing:FourBitColorBrush x:Key="ThemeChooserBackgroundBrush" Color="DarkCyan"/>
    <drawing:FourBitColorBrush x:Key="ThemeActionBackgroundBrush" Color="DarkGreen"/>
    <drawing:FourBitColorBrush x:Key="ThemeSelectionBackgroundBrush" Color="Blue"/>
    <drawing:FourBitColorBrush x:Key="ThemeSelectionForegroundBrush" Color="Black"/>
    <drawing:FourBitColorBrush x:Key="ThemeAlternativeBackgroundBrush" Color="Black"/>
    <drawing:FourBitColorBrush x:Key="ThemeBorderBrush" Color="Gray"/>
    <drawing:FourBitColorBrush x:Key="ThemeNoDisturbBrush" Color="Gray"/>
    <drawing:FourBitColorBrush x:Key="ThemeShadeBrush" Mode="Shaded"/>
    <drawing:FourBitColorBrush x:Key="ThemePseudoShadeBrush" Color="Black"/>
    <drawing:FourBitColorBrush x:Key="ThemeErrorBrush" Color="DarkRed"/>
</ResourceDictionary>