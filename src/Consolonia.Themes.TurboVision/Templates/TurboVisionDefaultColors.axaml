<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=System.Runtime"
                    xmlns:drawing="clr-namespace:Consolonia.Core.Drawing;assembly=Consolonia.Core">
    <!--https://www.mikrocontroller.net/attachment/483578/Turbo_Vision.gif-->
    <system:Double x:Key="FontSizeNormal">1</system:Double>
    <drawing:ConsoleColorBrush x:Key="ThemeForegroundBrush"
                               Color="Black" />
    <drawing:ConsoleColorBrush x:Key="ThemeBackgroundBrush"
                               Color="Gray" />
    <drawing:ConsoleColorBrush x:Key="ThemeChooserBackgroundBrush"
                               Color="Cyan" />
    <drawing:ConsoleColorBrush x:Key="ThemeActionBackgroundBrush"
                               Color="ForestGreen" />
    <drawing:ConsoleColorBrush x:Key="ThemeSelectionBackgroundBrush"
                               Color="DarkBlue" />
    <drawing:ConsoleColorBrush x:Key="ThemeSelectionForegroundBrush"
                               Color="White" />
    <drawing:ConsoleColorBrush x:Key="ThemeAlternativeBackgroundBrush"
                               Color="White" />
    <drawing:ConsoleColorBrush x:Key="ThemeBorderBrush"
                               Color="DarkGray" />
    <drawing:ConsoleColorBrush x:Key="ThemeNoDisturbBrush"
                               Color="DarkGray" />
    <drawing:ConsoleColorBrush x:Key="ThemeShadeBrush"
                               Mode="Shaded" />
    <drawing:ConsoleColorBrush x:Key="ThemePseudoShadeBrush"
                               Color="Black" />
    <drawing:ConsoleColorBrush x:Key="ThemeErrorBrush"
                               Color="Red" />
    <drawing:ConsoleColorBrush x:Key="ThemeTransparentBrush"
                               Mode="Transparent"/>
</ResourceDictionary>